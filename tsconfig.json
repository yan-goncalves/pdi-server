{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@data-source": ["src/data-source"],
      "@core/*": ["src/resources/core/*"],
      "@components/*": ["src/resources/components/*"],
      "@pages/*": ["src/resources/pages/*"],
      "@appModule": ["src/app.module"],
      "@decorators/*": ["src/decorators/*"],
      "@guards/*": ["src/guards/*"],
      "@middlewares/*": ["src/middlewares/*"],
      "@strategies/*": ["src/strategies/*"],
      "@auth/*": ["src/resources/core/auth/*"],
      "@ldap/*": ["src/resources/core/ldap/*"],
      "@constants/*": ["src/constants/*"],
      "@common/*": ["src/common/*"],
      "@users/*": ["src/resources/core/users/*"],
      "@users-info/*": ["src/resources/core/users-info/*"],
      "@departments/*": ["src/resources/core/departments/*"],
      "@departments-i18n/*": ["src/resources/core/departments-i18n/*"],
      "@sections/*": ["src/resources/core/sections/*"],
      "@sections-i18n/*": ["src/resources/core/sections-i18n/*"],
      "@questions/*": ["src/resources/core/questions/*"],
      "@questions-i18n/*": ["src/resources/core/questions-i18n/*"],
      "@skills/*": ["src/resources/core/skills/*"],
      "@skills-i18n/*": ["src/resources/core/skills-i18n/*"],
      "@feedbacks/*": ["src/resources/core/feedbacks/*"],
      "@feedbacks-i18n/*": ["src/resources/core/feedbacks-i18n/*"],
      "@evaluations/*": ["src/resources/core/evaluations/*"],
      "@evaluations-i18n/*": ["src/resources/core/evaluations-i18n/*"],
      "@goals/*": ["src/resources/core/goals/*"],
      "@kpis/*": ["src/resources/core/kpis/*"],
      "@evaluations-goals/*": ["src/resources/core/evaluations-goals/*"],
      "@evaluations-goals-kpis/*": ["src/resources/core/evaluations-goals-kpis/*"],
      "@pdi-coachings/*": ["src/resources/core/pdi-coachings/*"],
      "@pdi-qualities/*": ["src/resources/core/pdi-qualities/*"],
      "@pdi-competences/*": ["src/resources/core/pdi-competences/*"],
      "@pdi-competences-categories/*": ["src/resources/core/pdi-competences-categories/*"],
      "@pdi-competences-categories-i18n/*": [
        "src/resources/core/pdi-competences-categories-i18n/*"
      ],
      "@ratings/*": ["src/resources/core/ratings/*"],
      "@ratings-i18n/*": ["src/resources/core/ratings-i18n/*"],
      "@performed-evaluations/*": ["src/resources/core/performed-evaluations/*"],
      "@performed-questions/*": ["src/resources/core/performed-questions/*"],
      "@performed-skills/*": ["src/resources/core/performed-skills/*"],
      "@performed-goals/*": ["src/resources/core/performed-goals/*"],
      "@performed-goals-kpis/*": ["src/resources/core/performed-goals-kpis/*"],
      "@performed-feedbacks/*": ["src/resources/core/performed-feedbacks/*"],
      "@medias/*": ["src/resources/core/medias/*"],
      "@evaluation-result-concepts/*": ["src/resources/core/evaluation-result-concepts/*"],
      "@evaluation-result-concepts-i18n/*": ["src/resources/core/evaluation-result-concepts-i18n/*"],
    }
  }
}
